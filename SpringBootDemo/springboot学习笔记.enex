<span style="color: #9cdcfe; line-height: 160%; box-sizing: content-box;">      time-between-eviction-runs-millis:</span> <span style="color: #b8d7a3; line-height: 160%; box-sizing: content-box;">60000</span>
      <span style="color: #57a64a; font-style: italic; line-height: 160%; box-sizing: content-box;">#一个连接在池中最小生存的时间</span>
<span style="color: #9cdcfe; line-height: 160%; box-sizing: content-box;">      min-evictable-idle-time-millis:</span> <span style="color: #b8d7a3; line-height: 160%; box-sizing: content-box;">300000</span>
<span style="color: #9cdcfe; line-height: 160%; box-sizing: content-box;">      validation-query:</span> <span style="color: #d69d85; line-height: 160%; box-sizing: content-box;">SELECT</span> <span style="color: #d69d85; line-height: 160%; box-sizing: content-box;">'x'</span>
